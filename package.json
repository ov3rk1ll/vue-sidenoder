    "bootswatch": "^4.6.0",
    "command-exists": "^1.2.9",
    "core-js": "^3.9.1",
    "electron-settings": "^4.0.2",
    "electron-store": "^7.0.2",
    "flat": "^5.0.2",
    "mime-types": "^2.1.29",
    "node-apk-parser": "^0.2.3",
    "node-fetch": "^2.6.1",
    "open": "^8.0.2",
