    "@vue/cli-service": "~4.5.11",
    "babel-eslint": "^10.1.0",
    "electron": "^11.2.3",
    "electron-devtools-installer": "^3.1.1",
    "electron-icon-builder": "^2.0.1",
    "electron-unhandled": "^3.0.2",
    "eslint": "^8.13.0",
    "eslint-plugin-vue": "^7.7.0",
    "husky": "^5.1.3",
    "node-sass": "^5.0.0",
    "patch-package": "^6.4.6",
