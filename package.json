    "parserOptions": {
      "parser": "babel-eslint"
    },
    "rules": {},
    "ignorePatterns": [
      "**/dist",
      "**/dist_electron"
    ]
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
